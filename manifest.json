{
  "name": "flomo code",
  "version": "1.0",
  "manifest_version": 3,
  "description": "flomo md代码块高亮插件",
  "icons": {

    "128": "images/flomo128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://v.flomoapp.com/*"],
      "js": ["content.js","include/highlight.min.js"],
      "css": ["include/highlight.min.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["https://v.flomoapp.com/*"]
    }
  ]
}